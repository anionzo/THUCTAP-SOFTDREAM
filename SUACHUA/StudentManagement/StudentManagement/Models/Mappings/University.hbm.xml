<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="StudentManagement" namespace="StudentManagement.Models">
	
	<class name="University" table="TBL_University" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="IDUniversity" column="IDUniversity" type="string">
			<generator class="native" />
		</id>
		<property name="NameUniversity"  />
		<property name="Address" />
		<property name="PhoneNumber"  />
		<property name="Email" />
	</class>
	
</hibernate-mapping>